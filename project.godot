config_version=5

[application]

config/name="Ivory GUY"
run/main_scene="res://Assets/Scenes/title_screen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://img_icon.png"
config/windows_native_icon="res://img_icon.ico"

[autoload]

Level="*res://Assets/Scripts/Autoloads/Level.gd"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
SoundFx="*res://Assets/Content/Audio/sound_fx.tscn"
LibWeapon="*res://Assets/Scripts/Autoloads/lib_weapon.gd"
GameManager="*res://Assets/Scripts/Autoloads/GameManager.gd"
LibObject="*res://Assets/Scripts/Autoloads/lib_object.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="PixelHD"

[editor]

import/canvas_textures/default_texture_filter=1

[editor_plugins]

enabled=PackedStringArray("res://addons/SimpleTODO/plugin.cfg", "res://addons/beehave/plugin.cfg")

